    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "db-migrate-pg": "^1.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.2",
    "module-alias": "^2.2.0",
    "pg": "^7.9.0",
    "pm2": "^3.5.1",
